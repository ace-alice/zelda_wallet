{
  "typescript.preferences.autoImportFileExcludePatterns": ["vue-router"],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.debug": true,
  "eslint.format.enable": true,
  "eslint.ignoreUntitled": true,
  "eslint.experimental.useFlatConfig": false,
  // "eslint.nodePath": "/Users/Hector/.nvm/versions/node/v20.12.2/bin/eslint",
  "eslint.lintTask.enable": true,
  "eslint.problems.shortenToSingleLine": true,
  "eslint.quiet": true,
  "eslint.run": "onSave",
  "cSpell.words": [
    "aeskey",
    "brotli",
    "composables",
    "Creq",
    "cssnano",
    "Cssnext",
    "danmu",
    "encryptlong",
    "hextopem",
    "iconify",
    "jsrsasign",
    "Methodurl",
    "miniplayer",
    "miniprogress",
    "miniscreen",
    "naticpay",
    "nightwatch",
    "Nuvei",
    "Persistedstate",
    "pinia",
    "precomposed",
    "preinstall",
    "pxtorem",
    "rushstack",
    "stylelint",
    "swiper",
    "tsbuildinfo",
    "unplugin",
    "xgplayer"
  ],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
