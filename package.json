{
  "name": "zelda_wallet",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode fat --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:all": "vue-tsc --noEmit && vite build --mode fat && vite build --mode prod && vite build --mode uat",
    "serve": "http-server ./dist/fat -o --gzip",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "format": "prettier --write src/",
    "new": "plop",
    "svgo": "svgo -f src/assets/icons",
    "preinstall": "npx only-allow pnpm",
    "lint:tsc": "vue-tsc --noEmit",
    "lint:eslint": "eslint src/**/*.{ts,tsx,vue} --cache --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "postinstall": "simple-git-hooks && esno scripts/prepare.js"
  },
  "dependencies": {
    "@sumsub/websdk": "^2.3.4",
    "@types/crypto-js": "^4.2.2",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "default-passive-events": "^2.0.0",
    "element-plus": "^2.7.3",
    "encryptlong": "^3.1.4",
    "js-base64": "^3.7.7",
    "jsrsasign": "^11.1.0",
    "mitt": "^3.0.1",
    "nanoid": "^5.0.7",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.0.2",
    "scp2": "^0.5.0",
    "vue": "^3.4.27",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.15",
    "@iconify/vue": "^4.1.2",
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.11",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cssnano": "^7.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "plop": "^4.0.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.13",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.2.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-external-globals": "^0.12.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.1",
    "simple-git-hooks": "^2.11.1",
    "svgo": "^3.3.2",
    "typescript": "~5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-router": "^0.9.0",
    "vite": "^5.2.11",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-cdn2": "^1.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-pages": "^0.32.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.1.3",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^2.0.16"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  }
}
