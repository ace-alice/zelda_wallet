{
  "name": "zelda_wallet",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode fat --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:all": "vue-tsc --noEmit && vite build --mode fat && vite build --mode prod && vite build --mode uat",
    "serve": "http-server ./dist/fat -o --gzip",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "format": "prettier --write src/",
    "new": "plop",
    "svgo": "svgo -f src/assets/icons",
    "preinstall": "npx only-allow pnpm",
    "lint:tsc": "vue-tsc --noEmit",
    "lint:eslint": "eslint src/**/*.{ts,tsx,vue} --cache --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "postinstall": "simple-git-hooks && esno scripts/prepare.js"
  },
  "dependencies": {
    "@sumsub/websdk": "^2.3.16",
    "@types/crypto-js": "^4.2.2",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "default-passive-events": "^2.0.0",
    "element-plus": "^2.9.10",
    "encryptlong": "^3.1.4",
    "js-base64": "^3.7.7",
    "jsrsasign": "^11.1.0",
    "mitt": "^3.0.1",
    "nanoid": "^5.1.5",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "scp2": "^0.5.0",
    "vue": "^3.5.14",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.2.2",
    "@iconify/vue": "^5.0.0",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "cssnano": "^7.0.7",
    "eslint": "^9.27.0",
    "eslint-plugin-vue": "^10.1.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "npm-run-all2": "^8.0.3",
    "plop": "^4.0.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.6",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.5.3",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-external-globals": "^0.13.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.89.0",
    "simple-git-hooks": "^2.13.0",
    "svgo": "^3.3.2",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-cdn2": "^1.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-pages": "^0.33.0",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  }
}
